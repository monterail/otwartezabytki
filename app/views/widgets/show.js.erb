var _oz = {
  popup: function(url, title, w, h) {
    var left, targetWin, top;
    left = (screen.width / 2) - (w / 2);
    top = (screen.height / 2) - (h / 2);
    return targetWin = window.open(url, title, "toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=no,resizable=no,copyhistory=no,width=" + w + ",height=" + h + ",top=" + top + ",left=" + left);
  }
}
document.getElementById('oz_add_relic_widget').innerHTML = '<%= escape_javascript(render "/widgets/show/#{widget.widget_template.partial_name}")%>';

%section.feed.user-added-feed
  %h1.feed-title
    Dodane zabytki
    %span.badge 3

  %dl.feed-item
    %dd.feed-image.thumb
      %img{:alt => 'Zabytek', :src => asset_path('homepage-image-example.png'), :width => 56, :height => 56}
    %dt.feed-message
      %a{:href => '#'}> Most Grunwaldzki
    %dd.feed-message
      %b.status.checked> Sprawdzony

  %dl.feed-item
    %dd.feed-image.thumb
      %img{:alt => 'Zabytek', :src => asset_path('fallback/photo_icon_default.png'), :width => 56, :height => 56}
    %dt.feed-message
      %a{:href => '#'}> Panorama Racławicka
    %dd.feed-message
      %b.status.unchecked> Niesprawdzony

  %dl.feed-item
    %dd.feed-image.thumb
      %img{:alt => 'Zabytek', :src => asset_path('fallback/photo_icon_default.png'), :width => 56, :height => 56}
    %dt.feed-message
      %a{:href => '#'}> Most Tumski
    %dd.feed-message
      %b.status.filled> Uzupełniony

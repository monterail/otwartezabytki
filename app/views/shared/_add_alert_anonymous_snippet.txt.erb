<script type="text/javascript">
(function(w, d, s, x){
  d = w.document;
  function OZ_async_load(){
    s = d.createElement('script');
    s.type = 'text/javascript'; s.async = !0;
    s.src = '//<%= Settings.oz.host %>/widgets/add_alert.js?relic_id=<%= id %>';
    x = d.getElementsByTagName('script')[0];
    x.parentNode.insertBefore(s, x);
  }
  if (w.addEventListener) w.addEventListener('load', OZ_async_load, !1);
  else if (w.attachEvent) w.attachEvent('onload', OZ_async_load);
})(window);
</script>
<div id='oz_add_alert_widget'></div>
